{
    "github": {
		"silent": true
	},
    "rewrites": [
        {
            "source": "/love",
            "destination": "/web/"
        },
	    {
            "source": "/blog/(.*)",
            "destination": "/blog/index.html",
			 "headers": [
				{
				  "key": "Access-Control-Allow-Origin",
				  "value": "*"
				}
			]
        },
	    {
            "source": "/qqapi/(.*)",
			"destination": "https://graph.qq.com"
        }
    ]
}
