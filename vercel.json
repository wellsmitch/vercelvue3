{
    "github": {
		"silent": true
	},
    "rewrites": [
        {
            "source": "/love",
            "destination": "/web/"
        },
	    {
            "source": "/blog/(.*)",
            "destination": "/blog/index.html"
        },
	    {
            "source": "/qqapi/(.*)",
			"destination": "https://graph.qq.com"
        }
    ],
	"headers": [
		{
		  "source": "/(.*)",
		  "headers": [
			{
			  "key": "access-control-allow-origin",
			  "value": "*"
			}
		  ]
		}
	  ]
}
